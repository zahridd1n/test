    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, natija, i" type="Integer" array="False" size=""/>
            <output expression="&quot;a ni kiriting:&quot;" newline="True"/>
            <input variable="a"/>
            <output expression="&quot;b ni kiriting:&quot;" newline="True"/>
            <input variable="b"/>
            <assign variable="natija" expression="0"/>
            <if expression="a&lt;b">
                <then>
                    <for variable="i" start="a" end="b" direction="inc" step="1">
                        <assign variable="natija" expression="natija+i^2"/>
                    </for>
                    <output expression="natija" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Xatolik! b , a dan katta bolsin&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
